<div class="container mt-3">
  <div class="row">
    <div class="col-md-10 mx-auto">
      <h3>商品一覧</h3>
    </div>
    <div class="col">
      <%= link_to '+', new_admin_item_path, class: "btn btn-success rounded-circle" %>
    </div>
  </div>
  <div class="row">
    <div class="col">
      <div class="items-all">
        <table class="table">
          <thead>
            <tr>
              <th style="width: 10%">商品ID</th>
              <th style="width: 30%">商品名</th>
              <th style="width: 10%">税抜価格</th>
              <th style="width: 30%">ジャンル</th>
              <th style="width: 20%">ステータス</th>
            </tr>
          </thead>
          <tbody>
            <%= render 'index', items: @items %>
          </tbody>
        </table>
      </div>

    </div>
  </div>
</div>